<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox xmlns="http://javafx.com/javafx/8.0.261" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.WelcomeWindowController">
	<Pane onKeyPressed="#onSubmitPressedKEy" prefHeight="385.0" prefWidth="669.0" style="-fx-background-color: #222d40;">
      <children>
            <Text fx:id="welcomeText" fill="#ffffff" layoutX="98.0" layoutY="124.0">
               <font>
                  <Font name="Calibri Bold" size="29.0" />
               </font>
            </Text>
            <Button fx:id="submitButton" layoutX="290.0" layoutY="270.0" onAction="#onSubmit" prefHeight="19.0" prefWidth="112.0" style="-fx-background-color: #58c6c7;" text="SUBMIT" textFill="#ffffff">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
           	</Button>
            <ComboBox fx:id="mtComboBox" layoutX="167.0" layoutY="180.0" onAction="#onchooseMT" prefHeight="31.0" prefWidth="357.0" promptText="--Please select an option--" style="-fx-background-color: #FFFFFF;" />
      </children>
    </Pane>
</VBox>
